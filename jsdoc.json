{
    "source": {
        "include": [
			"./src"
		],
		"includePattern": "\\.js$",
		"excludePattern": "(node_modules\\/|docs)"
    },
    "templates": {
		"default": {
			"outputSourceFiles": false,
			"includeDate": false
		},
		"monospaceLinks": true
    },
	"plugins": [
		"plugins/markdown",
		"node_modules/jsdoc-tsimport-plugin/index.js"
	],
	"opts": {
		"destination": "./docs/",
		"recurse": true,
		"readme": "README.md"
	}
}